{
	"name": "@kubosho/blog-client",
	"version": "10.0.19",
	"license": "MIT",
	"private": true,
	"scripts": {
		"dev": "astro dev",
		"build": "astro build",
		"build:node": "USE_NODE_ADAPTER=true astro build",
		"build:ogimage": "tsx ./tools/og_image/generate_og_image.ts",
		"preview": "astro preview",
		"preview:node": "USE_NODE_ADAPTER=true astro preview",
		"test": "vitest run",
		"test:e2e": "npx playwright test",
		"lint:style": "stylelint **/*.css",
		"lint:script": "eslint .",
		"lint:markup": "markuplint **/*.astro",
		"format": "prettier --write .",
		"check:format": "npm run format && git diff --exit-code",
		"check:ts": "tsc --project tsconfig.json --noEmit"
	},
	"dependencies": {
		"@astrojs/cloudflare": "11.2.0",
		"@astrojs/sitemap": "3.3.1",
		"astro": "4.16.18",
		"dayjs": "1.11.13",
		"escape-html": "1.0.3",
		"prismjs": "1.30.0",
		"remark-breaks": "4.0.0",
		"remark-gfm": "4.0.1",
		"remark-html": "16.0.1",
		"remark-parse": "11.0.0",
		"remark-stringify": "11.0.0",
		"rosetta": "1.1.0",
		"strip-markdown": "6.0.0",
		"unified": "11.0.5"
	},
	"devDependencies": {
		"@astrojs/node": "8.3.4",
		"@kubosho/configs": "4.0.32",
		"@markuplint/astro-parser": "4.6.19",
		"@playwright/test": "1.52.0",
		"@types/escape-html": "1.0.4",
		"@types/node": "22.15.3",
		"@types/prismjs": "1.26.5",
		"eslint-plugin-astro": "1.3.1",
		"fishery": "2.3.0",
		"front-matter": "4.0.2",
		"jsdom": "26.1.0",
		"prettier": "3.5.3",
		"prettier-plugin-astro": "0.14.1",
		"sharp": "0.34.1",
		"typescript": "5.8.3",
		"vitest": "3.1.2"
	}
}
