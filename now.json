{
  "version": 2,
  "name": "I'm kubosho",
  "build": {
    "env": {
      "SPACE": "@space",
      "ACCESS_TOKEN": "@access_token"
    }
  },
  "regions": ["hnd1", "icn1", "sfo1"],
  "routes": [
    { "src": "/feed", "dest": "/api/feed" },
    {
      "src": "/api/feed",
      "status": 308,
      "headers": { "Location": "/feed" }
    }
  ]
}
