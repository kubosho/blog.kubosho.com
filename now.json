{
  "version": 2,
  "name": "I'm kubosho",
  "build": {
    "env": {
      "SPACE": "@space",
      "ACCESS_TOKEN": "@access_token",
      "BUGSNAG_API_KEY": "@bugsnag_api_key"
    }
  },
  "routes": [
    { "src": "/feed", "dest": "/api/feed" },
    {
      "src": "/api/feed",
      "status": 308,
      "headers": { "Location": "/feed" }
    },
    {
      "src": "/feed.xml",
      "status": 308,
      "headers": { "Location": "/feed" }
    }
  ]
}
