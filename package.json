{
  "name": "blog-kubosho-com",
  "version": "6.19.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "staging": "vercel dev",
    "build": "next build",
    "start": "next start",
    "test": "vitest run",
    "update_snapshot": "jest --updateSnapshot",
    "bundle_analyze": "BUNDLE_ANALYZE=true next build",
    "style_lint": "stylelint **/*.tsx",
    "script_lint": "eslint --config .eslintrc.js --ext .ts,.tsx .",
    "markup_lint": "markuplint **/*.tsx",
    "format": "prettier --config .prettierrc.js --write **/*.{js,jsx,ts,tsx}",
    "check_format": "npm run format && git diff --exit-code",
    "check_ts": "tsc --project tsconfig.json --noEmit",
    "staging_deploy": "vercel --confirm --token ${ZEIT_TOKEN}",
    "prod_deploy": "vercel --confirm --token ${ZEIT_TOKEN} --prod"
  },
  "dependencies": {
    "@bugsnag/js": "^7.13.2",
    "@bugsnag/plugin-react": "^7.13.2",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "escape-html": "^1.0.3",
    "next": "^12.1.2",
    "option-t": "^28.2.0",
    "prismjs": "^1.27.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@mapbox/rehype-prism": "^0.8.0",
    "@markuplint/jsx-parser": "^2.3.0",
    "@markuplint/react-spec": "^2.1.0",
    "@next/bundle-analyzer": "^12.0.1",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.1",
    "@types/escape-html": "^1.0.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^17.0.0",
    "@types/prismjs": "^1.16.6",
    "@types/react": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "@vitejs/plugin-react": "^1.3.2",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "front-matter": "^4.0.2",
    "jest": "^27.3.1",
    "jsdom": "^19.0.0",
    "markuplint": "^2.6.2",
    "prettier": "^2.4.1",
    "prettier-plugin-md-nocjsp": "^1.2.0",
    "react-test-renderer": "^17.0.2",
    "rehype": "^12.0.0",
    "rehype-stringify": "^8.0.0",
    "remark-breaks": "^2.0.2",
    "remark-gfm": "^1.0.0",
    "remark-parse": "^9.0.0",
    "remark-prism": "^1.3.6",
    "remark-rehype": "^8.1.0",
    "remark-stringify": "^9.0.1",
    "scaffdog": "^1.0.1",
    "semantic-release": "^18.0.0",
    "strip-markdown": "^4.2.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4",
    "unified": "^10.1.2",
    "vercel": "^23.1.2",
    "vitest": "^0.12.9"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/changelog",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  }
}
