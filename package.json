    "front-matter": "^4.0.2",
    "jsdom": "^24.1.1",
    "markuplint": "^4.9.2",
    "prettier-plugin-astro": "^0.14.1",
    "semantic-release": "^24.0.0",
    "stylelint": "^16.7.0",
    "stylelint-config-recommended": "^14.0.1",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
