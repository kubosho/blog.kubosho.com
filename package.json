{
  "name": "blog-kubosho-com",
  "version": "6.34.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "staging": "vercel dev",
    "build": "next build",
    "start": "next start",
    "test": "vitest run",
    "bundle_analyze": "BUNDLE_ANALYZE=true next build",
    "style_lint": "stylelint **/*.css",
    "script_lint": "eslint --config .eslintrc.js --ext .ts,.tsx .",
    "markup_lint": "markuplint **/*.tsx",
    "format": "prettier --config .prettierrc.js --write **/*.{js,jsx,ts,tsx}",
    "check_format": "npm run format && git diff --exit-code",
    "check_ts": "tsc --project tsconfig.json --noEmit",
    "staging_deploy": "vercel --confirm --token ${ZEIT_TOKEN}",
    "prod_deploy": "vercel --confirm --token ${ZEIT_TOKEN} --prod"
  },
  "dependencies": {
    "@bugsnag/js": "^7.17.0",
    "@bugsnag/plugin-react": "^7.17.0",
    "dayjs": "^1.11.4",
    "dotenv": "^16.0.1",
    "escape-html": "^1.0.3",
    "next": "^12.2.4",
    "option-t": "^32.5.0",
    "prismjs": "^1.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rosetta": "^1.1.0"
  },
  "devDependencies": {
    "@mapbox/rehype-prism": "^0.8.0",
    "@markuplint/jsx-parser": "^2.4.2",
    "@markuplint/react-spec": "^2.1.1",
    "@next/bundle-analyzer": "^12.2.4",
    "@next/eslint-plugin-next": "^12.2.4",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.1",
    "@types/escape-html": "^1.0.2",
    "@types/node": "^18.6.4",
    "@types/prismjs": "^1.26.0",
    "@types/react": "^18.0.16",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@vitejs/plugin-react": "^1.3.2",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "front-matter": "^4.0.2",
    "jsdom": "^20.0.0",
    "markuplint": "^2.10.0",
    "prettier": "^2.7.1",
    "prettier-plugin-md-nocjsp": "^1.3.0",
    "react-test-renderer": "^18.2.0",
    "rehype": "^12.0.1",
    "rehype-stringify": "^9.0.3",
    "remark-breaks": "^3.0.2",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-prism": "^1.3.6",
    "remark-rehype": "^10.1.0",
    "remark-stringify": "^10.0.2",
    "scaffdog": "^1.5.0",
    "semantic-release": "^19.0.3",
    "strip-markdown": "^5.0.0",
    "stylelint": "^14.9.1",
    "stylelint-config-recommended": "^8.0.0",
    "typescript": "^4.7.4",
    "unified": "^10.1.2",
    "vercel": "^27.2.0",
    "vitest": "^0.21.0"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/changelog",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  }
}
