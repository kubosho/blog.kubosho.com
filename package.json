    "check:format": "npm run format && git diff --exit-code",
    "check:ts": "tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^11.0.1",
    "@astrojs/sitemap": "^3.1.6",
    "astro": "^4.13.1",
    "dayjs": "^1.11.12",
    "escape-html": "^1.0.3",
    "prismjs": "^1.29.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.0",
    "remark-html": "^16.0.1",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "rosetta": "^1.1.0",
    "strip-markdown": "^6.0.0",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@astrojs/node": "^8.3.2",
    "@kubosho/configs": "^3.9.0",
    "@playwright/test": "^1.45.3",
