  "private": true,
  "dependencies": {
    "@bugsnag/js": "^7.5.4",
    "@bugsnag/plugin-react": "^7.3.1",
    "dayjs": "^1.9.8",
    "next": "^10.0.4",
    "option-t": "^24.1.0",
    "prismjs": "^1.22.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@mapbox/rehype-prism": "^0.5.0",
    "@next/bundle-analyzer": "^10.0.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.16",
    "@types/prismjs": "^1.16.2",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "front-matter": "^4.0.2",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "rehype": "^11.0.0",
    "rehype-stringify": "^8.0.0",
    "remark-breaks": "^2.0.1",
    "remark-gfm": "^1.0.0",
    "remark-parse": "^9.0.0",
    "remark-prism": "^1.3.3",
    "remark-rehype": "^8.0.0",
    "remark-stringify": "^9.0.1",
    "semantic-release": "^17.3.0",
    "strip-markdown": "^4.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended": "^3.0.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3",
    "unified": "^9.2.0",
    "vercel": "^21.0.1"
  },
  "scripts": {
    "dev": "next dev",
    "staging": "vercel dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "update_snapshot": "jest --updateSnapshot",
    "bundle_analyze": "BUNDLE_ANALYZE=true next build",
    "style_lint": "stylelint **/*.tsx",
    "script_lint": "eslint --config .eslintrc.js --ext .ts,.tsx .",
    "format": "prettier --config .prettierrc.js --write **/*.{js,jsx,ts,tsx}",
    "check_format": "npm run format && git diff --exit-code",
    "check_ts": "tsc --project tsconfig.json --noEmit",
    "staging_deploy": "vercel --confirm --token ${ZEIT_TOKEN}",
    "prod_deploy": "vercel --confirm --token ${ZEIT_TOKEN} --prod"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/changelog",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  }
}
