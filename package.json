{
  "name": "blog-kubosho-com",
  "version": "7.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "export": "ON_NEXT_BUILD=true next build && next export",
    "dev": "next dev",
    "staging": "vercel dev",
    "build": "next build",
    "postbuild": "next-sitemap --config tools/next-sitemap.config.js",
    "start": "next start",
    "test": "vitest run",
    "bundle_analyze": "BUNDLE_ANALYZE=true next build",
    "style_lint": "stylelint **/*.css",
    "script_lint": "eslint --config .eslintrc.js --ext .ts,.tsx .",
    "markup_lint": "markuplint **/*.tsx",
    "format": "prettier --config .prettierrc.js --write **/*.{js,jsx,ts,tsx}",
    "check_format": "npm run format && git diff --exit-code",
    "check_ts": "tsc --project tsconfig.json --noEmit",
    "staging_deploy": "vercel --confirm --token ${ZEIT_TOKEN}",
    "prod_deploy": "vercel --confirm --token ${ZEIT_TOKEN} --prod"
  },
  "dependencies": {
    "@bugsnag/js": "^7.18.0",
    "@bugsnag/plugin-react": "^7.18.0",
    "@headlessui/react": "^1.7.7",
    "@mapbox/rehype-prism": "^0.8.0",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "escape-html": "^1.0.3",
    "next": "^13.1.2",
    "next-sitemap": "^3.1.45",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype": "^12.0.1",
    "rehype-stringify": "^9.0.3",
    "remark-breaks": "^3.0.2",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-prism": "^1.3.6",
    "remark-rehype": "^10.1.0",
    "remark-stringify": "^10.0.2",
    "rosetta": "^1.1.0",
    "strip-markdown": "^5.0.0",
    "unified": "^10.1.2"
  },
  "devDependencies": {
    "@kubosho/eslint-config": "^2.0.3",
    "@markuplint/jsx-parser": "^3.1.0",
    "@markuplint/react-spec": "^2.1.2",
    "@next/bundle-analyzer": "^13.1.2",
    "@next/eslint-plugin-next": "^13.1.2",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@types/escape-html": "^1.0.2",
    "@types/node": "^18.11.18",
    "@types/prismjs": "^1.26.0",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@vitejs/plugin-react": "^1.3.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.0",
    "front-matter": "^4.0.2",
    "jsdom": "^21.0.0",
    "markuplint": "^3.1.0",
    "prettier": "^2.8.3",
    "prettier-plugin-md-nocjsp": "^1.5.0",
    "react-test-renderer": "^18.2.0",
    "semantic-release": "^20.0.2",
    "stylelint": "^14.16.1",
    "stylelint-config-recommended": "^9.0.0",
    "typescript": "^4.9.4",
    "unified": "^10.1.2",
    "vercel": "^28.12.4",
    "vitest": "^0.27.1"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/changelog",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  }
}
